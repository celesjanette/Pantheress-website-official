{
  "name": "wu-tang-react-node-template",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "babel-core": "6.26.3",
    "body-parser": "1.20.3",
    "dotenv": "16.4.5",
    "express": "4.21.0",
    "express-jwt": "8.4.1",
    "lodash": "4.17.21",
    "path": "0.12.7",
    "puppeteer": "^23.4.1"
  },
  "devDependencies": {
    "concurrently": "9.0.1",
    "nodemon": "3.1.7"
  },
  "cacheDirectories": [
    "node_modules",
    "src/frontend/node_modules"
  ],
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "nodemon src/backend",
    "frontend": "nodemon start-frontend.js",
    "prod-backend": "node --expose-gc --inspect --optimize_for_size --max_old_space_size=460 --gc_interval=100 src/backend",
    "heroku-postbuild": "npm install react-scripts && cd src/frontend && npm install && npm run build"
  }
}
