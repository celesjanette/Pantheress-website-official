{
  "name": "wu-tang-react-node-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-core": "^6.26.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jwks-rsa": "^1.4.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.19",
    "path": "^0.12.7",
    "puppeteer": "^1.13.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10"
  },
  "cacheDirectories": [
    "node_modules",
    "src/client/node_modules"
  ],
  "engines": {
    "node": "=10.13.0",
    "npm": "=6.4.1"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon src/server/server.js",
    "prod-server": "node src/server/server.js",
    "client": "nodemon start-client.js",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1",
    "lint": "eslint .",
    "heroku-postbuild": "cd src/client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  }
}
